      "post":{
        "tags":[
          "verification-controller"
        ],
        "operationId":"createTeleTAN",
        "responses":{
          "200":{
            "description":"OK",
            "content":{
              "*/*":{
                "schema":{
                  "type":"string"
                }
              }
            }
          }
        }
      }
    }
