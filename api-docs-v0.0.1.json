          "registrationToken":{
            "type":"string"
          }
        }
      },
      "TanRequest":{
        "type":"object",
        "properties":{
          "key":{
            "type":"string"
          },
          "keyType":{
            "type":"string",
            "enum":[
              "TOKEN",
              "TELETAN"
            ]
          }
        }
      },
      "Tan":{
        "type":"object",
        "properties":{
          "tan":{
            "type":"string"
          }
        }
      },
      "TestResult":{
        "type":"object",
        "properties":{
          "testResult":{
            "type":"integer",
            "format":"int32"
          }
        }
