            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/HashedGuid"
              }
            }
          },
          "required":true
        },
        "responses":{
          "200":{
            "description":"OK",
            "content":{
              "*/*":{
                "schema":{
                  "$ref":"#/components/schemas/RegistrationToken"
                }
              }
            }
          }
        }
      }
    },
    "/version/v1/tan":{
      "post":{
        "tags":[
          "verification-controller"
        ],
        "operationId":"generateTAN",
        "requestBody":{
          "content":{
            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/TanRequest"
              }
            }
          },
          "required":true
        },
        "responses":{
          "200":{
            "description":"OK",
            "content":{
              "*/*":{
                "schema":{
                  "$ref":"#/components/schemas/Tan"
                }
              }
            }
          }
        }
      }
