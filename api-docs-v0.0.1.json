      "post":{
        "tags":[
          "verification-controller"
        ],
        "operationId":"verifyTAN",
        "requestBody":{
          "content":{
            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/Tan"
              }
            }
          },
          "required":true
        },
        "responses":{
          "200":{
            "description":"OK"
          }
        }
      }
    },
